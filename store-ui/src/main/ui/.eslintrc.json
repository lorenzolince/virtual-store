{
    "root": true,
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "prettier/vue",
    "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "$nuxt": true
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2018
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-console": 0,
        "new-cap": 0,
        "no-unused-vars": 0,
        "no-debugger": 0
    }
}
